{
  "name": "chat-ptt-nes.io",
  "version": "1.0.0",
  "description": "a concept run of a chat with push to talk in node, express, sequelize, socket.io",
  "main": "bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "prod": "forever start ./bin/www",
    "preinstall": "cp -fv config/db.example config/db.json && cp -fv config/env.example config/env.json",
    "postinstall": "bower install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/virgenherrera/chat-ptt-nes.io.git"
  },
