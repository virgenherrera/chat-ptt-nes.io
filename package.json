  "bugs": {
    "url": "https://github.com/virgenherrera/chat-ptt-nes.io/issues"
  },
  "homepage": "https://github.com/virgenherrera/chat-ptt-nes.io#readme",
  "dependencies": {
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "morgan": "~1.7.0",
    "mysql": "^2.13.0",
    "pug": "~2.0.0-beta10",
    "sequelize": "^3.30.4",
    "serve-favicon": "~2.3.2",
    "socket.io": "^1.7.3"
  },
  "devDependencies": {
    "sequelize-cli": "^2.7.0"
